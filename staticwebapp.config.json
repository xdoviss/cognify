{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "get",
        "post",
        "head",
        "put",
        "connect",
        "delete",
        "options",
        "patch",
        "trace"
      ],
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/WordRecall/*"
    },
    {
      "route": "/api/WordRecall/start"
    },
    {
      "route": "/api/WordRecall/update-state"
    },
    {
      "route": "/api/WordRecall/update-score"
    },
    {
      "route": "/api/WordRecall/update-health"
    },
    {
      "route": "/api/WordRecall/highscore"
    },
    {
      "route": "/api/WordRecall/is-finished"
    }
  ]
}